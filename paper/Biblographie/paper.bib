issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {RNA splicing,batch effects,leukemia,nonsense-mediated decay},
month = {nov},
number = {47},
pages = {16802--7},
pmid = {25385641},
publisher = {National Academy of Sciences},
title = {{Sample processing obscures cancer-specific alterations in leukemic transcriptomes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25385641 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4250124},
volume = {111},
year = {2014}
}
