title = {{Higher gene expression variability in the more aggressive subtype of chronic lymphocytic leukemia.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25632304 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4308895},
volume = {7},
year = {2015}
}
