journal = {Genome biology},
number = {10},
pages = {R106},
pmid = {20979621},
publisher = {BioMed Central},
title = {{Differential expression analysis for sequence count data.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20979621 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3218662},
