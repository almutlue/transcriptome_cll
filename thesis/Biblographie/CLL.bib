title = {{Genome-wide analysis of differential transcriptional and epigenetic variability across human immune cell types.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28126036 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5270224},
volume = {18},
